<h3><a class="btn btn-secondary" href="/phone"><i class="fa fa-plus"></i>Create New</a>Phone List</h3>
<table class="table table-striped">
    <thread>
        <tr>
            <th>Full Name</th>
            <th>Email</th>
            <th>Mobile</th>
            <th></th>
        </tr>
    </thread>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.fullName}}</td>
            <td>{{this.Email}}</td>
            <td>{{this.Mobile}}</td>
            <td>
                <a href="/phone/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
                <a href="/phone/delete/{{this._id}}" onclick="return confirm('Are you sure to delete this record?');"><i
                        class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>